<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
  <remote name="SHRP-Reborn"
	fetch="https://github.com/SHRP-Reborn" />

  <remote name="devices" 
	fetch="https://github.com/SHRP-Devices" />

  <!-- Sync our own bootable/recovery -->
  <remove-project name="platform/bootable/recovery"/>

  <project path="bootable/recovery" name="bootable_recovery" remote="SHRP-Reborn" revision="android-9.0"/>
  <project path="vendor/shrp" name="vendor_shrp" remote="SHRP-Reborn" revision="android-9.0"/>
  <project path="system/update_engine" name="android_system_update_engine" remote="SHRP-Reborn" revision="android-9.0"/>
  
  <!-- Busybox repo -->
  <project path="external/busybox" name="android_external_busybox" remote="omnirom" revision="android-9.0" />

  <!-- Magiskboot repo -->
  <project path="external/magisk-prebuilt" name="external_magisk-prebuilt" remote="SHRP-Reborn" revision="android-9.0" />

</manifest>
